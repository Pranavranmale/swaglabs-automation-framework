{
  "compilerOptions": {
    "module": "esnext",
    "target": "es6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@utils/*": ["src/Utils/*"],
      "@fixtures/*": ["src/Fixtures/*"],
      "@pages/*": ["src/PageObject/*"]
    }
  }
}
